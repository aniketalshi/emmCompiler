  int f = (b/4);
  int g = (10/2);
  int h = (10%2);
  int i = (1|(1&0));
  int j = (0&1);
  int k = 1^1;
  int l = (1<<2);
  int m = (100>>2);
  int bar(int aa) {
    
    return (aa+1);
  };
  int foo(int a) {
    
    a=10;
    if (a==1)     a=2;
    else     a=((3+bar(a))+bar(a)*2);
    return a;
  };
 

