event a(int aa, int bb);

int fac (int a) {
    if (a <= 1) return 1;
    return a * fac(a-1);
}

int bar (int a) {
    bool b = true, c=false;
    if(b && c)
        a=100;
    else
        a=99;
    return a;
}

a(i, j)-> {
    print("\\nFirst Param :", i);
    i = fac(i);
    print("\\nResult(fac) :", i);

    print("\\nSecond Param :", j);
    j = fac(j);
    print("\\nResult(fac) :", j);
}

