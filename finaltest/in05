// test bitnot 
	int b = 16;
	unsigned int bitnot(unsigned int arg) {
		unsigned int i=0;
		unsigned int ret=0;
		while(i<32) {
			if ( ((arg>>i) & 1) == 0 ) {
				ret = ret + (1 << i);
			}
			i = i +1;
		}
		return ret;
	}
	void test() {
	 	int a = 4;
		print (~a);
		print "\\n";
		print bitnot(a);
		print "\\n";
		while(1) {print "";};
	}
    event o(string a1 , int a2 , unsigned int a3);
