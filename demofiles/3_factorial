//Nested file and recursion

event a(int aa, int bb);
int aa = 2;

int factorial(int x)
{
    if(x<=1)
        return 1;
    else if (x==2)
	return factorial(x-1)*x;
    else return factorial(x-2) + factorial(x-1);
}

a(i, j)-> {
    i = factorial(i);
    print("\\nFactorial of 1st Param: ", i );

    j = factorial(j);
    print("\\nFactorial of 2nd Param: ", j );
}

